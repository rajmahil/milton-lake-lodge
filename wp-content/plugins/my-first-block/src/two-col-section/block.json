{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "brad-boilerplate/two-col-section",
	"version": "0.1.0",
	"title": "Two Column Section",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"topHeading": {
			"type": "string",
			"selector": "p"
		},
		"heading": {
			"type": "string",
			"selector": "h1"
		},
		"text": {
			"type": "string",
			"selector": "p"
		},
		"buttonText": {
			"type": "string",
			"default": "Learn More"
		},
		"buttonUrl": {
			"type": "string",
			"default": "#"
		},
		"image": {
			"type": "object",
			"default": {}
		},
		"image2": {
			"type": "object",
			"default": {}
		},
		"inverted": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true
		}
	},
	"textdomain": "two-col-section",
	"editorScript": "file:./index.js",
	"editorStyle": [ "file:../style-index.css" ],
	"style": [ "file:../style-index.css" ],
	"render": "file:./render.php"
}
