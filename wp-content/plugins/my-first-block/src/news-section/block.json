{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "brad-boilerplate/news-section",
	"version": "0.1.0",
	"title": "News Section",
	"category": "widgets",
	"icon": "smiley",
	"description": "News section",
	"example": {},
	"attributes": {
		"sortBy": {
			"type": "string",
			"default": "newest"
		},
		"sectionId": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true
		}
	},
	"textdomain": "news-section",
	"editorScript": "file:./index.js",
	"editorStyle": [ "file:../style-index.css" ],
	"style": [ "file:../style-index.css" ],
	"render": "file:./render.php"
}
