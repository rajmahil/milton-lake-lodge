{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "brad-boilerplate/form-block",
	"version": "0.1.0",
	"title": "Form Section",
	"category": "widgets",
	"icon": "smiley",
	"example": {},
	"attributes": {
		"heading": {
			"type": "string",
			"default": "Your Adventure Awaits"
		},
		"subheading": {
			"type": "string",
			"default": "You subheading goes"
		},
		"formTitle": {
			"type": "string",
			"default": "Contact Us"
		},
		"fields": {
			"type": "array",
			"default": []
		},
		"submitButtonText": {
			"type": "string",
			"default": "Send Message"
		},
		"sectionId": {
			"type": "string",
			"default": ""
		}
	},

	"supports": {
		"html": false
	},
	"textdomain": "form-section-block",
	"editorScript": "file:./index.js",
	"editorStyle": [ "file:../style-index.css" ],
	"style": [ "file:../style-index.css" ],
	"render": "file:./render.php",
	"viewScript": "file:./frontend.js"
}
