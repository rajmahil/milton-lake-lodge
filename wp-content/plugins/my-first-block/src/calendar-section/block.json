	"apiVersion": 3,
	"name": "brad-boilerplate/calendar-section",
	"version": "0.1.0",
	"title": "Calendar Section",
	"category": "widgets",
	"icon": "smiley",
	"description": "Calendar section",
	"example": {},
	"attributes": {
